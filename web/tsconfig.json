{
  "compilerOptions": {
    "outDir": "dist",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "types": [
      "@aeria-ui/ui/vue",
      "unplugin-vue-router/client"
    ]
  },
  "include": [
    "@types/*.d.ts",
    "src",
    ".aeria-ui/*.d.ts"
  ]
}
